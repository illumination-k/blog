{
  "name": "next-blog",
  "version": "1.0.0",
  "main": "index.tsx",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "yarn cache-posts test && jest",
    "cov": "yarn cache-posts test && jest --collect-coverage",
    "make-sitemap": "node sitemap.js",
    "cache-posts": "node script/makePostsCache.js",
    "preprocessing": "yarn cache-posts && git add cache/data.json cache/data.js && yarn build && yarn make-sitemap && git add public/sitemap.xml && git commit -m 'preprocess' --no-verify",
    "analyzer": "ANALYZE=true next build && cd .next/analyze && python -m http.server"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn preprocessing"
    }
  },
  "author": "illumination-k",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mdx-js/mdx": "^2.1.1",
    "@mui/icons-material": "^5.0.1",
    "@mui/lab": "^5.0.0-alpha.48",
    "@mui/material": "^5.6.0",
    "@mui/styles": "^5.6.0",
    "axios": "^0.24.0",
    "blog-remark": "0.0.11",
    "js-yaml": "^4.1.0",
    "next": "^12.1.5-canary.4",
    "next-seo": "^5.4.0",
    "nextjs-sitemap-generator": "^1.1.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.1",
    "remark": "14.0.2",
    "remark-footnotes": "^4.0.1",
    "remark-frontmatter": "^2.0.0",
    "remark-gfm": "^3.0.0",
    "remark-parse": "^10.0.1",
    "remark-stringify": "^9.0.1",
    "unified": "^10.1.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.2",
    "@types/material-ui": "^0.21.8",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.5-canary.4",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^27.0.7",
    "typescript": "^4.7.0-beta"
  },
  "browser": {
    "fs": false
  }
}
